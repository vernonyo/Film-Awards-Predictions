    "#creating a new column\n",
    "sales.insert(4, \"Release Year\", 0)\n",
    "#Moving the 'Release Year' from the 'Title' to the 'Release Year' column\n",
    "new = sales[\"Title\"].str.split(\"(\", n = 1, expand = True)\n",
    "sales[\"Title\"]= new[0]\n",
    "#Remove the brackets\n",
    "temp = new[1].str.split(\")\", n = 1, expand = True)\n",
    "sales[\"Release Year\"]= temp[0].astype(str).astype('int64')\n",
    "#Remove the last whitespace\n",
    "sales['Title'] = sales['Title'].str.strip()"
